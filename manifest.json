{
  "manifest_version": 2,
  "name": "Maliyas Extension",
  "description": "attempt at browser extension",
  "version": "0.1",
  "content_scripts": [
    {
      "matches": [
        "*://*/*",
        "file://*"
      ],
      "js": [
        "content.js"
      ],
      "run_at": "document_idle"
    }
  ],
  "icons": {
   "128": "images/icon.jpg"
  }
}
